par(mfrow=c(2,2))
par(mar=c(4,4,1,1))
plot(DF10$DFdate,DF10$Global_active_power,type="l",ylab="Global active power",xlab="")
plot(DF10$DFdate,DF10$Voltage,type="l",ylab="Voltage",xlab="datetime")
plot(DF10$DFdate,DF10$Sub_metering_1,type="l",ylab="Energy sub metering")
lines(DF10$DFdate,DF10$Sub_metering_2,col="red")      
lines(DF10$DFdate,DF10$Sub_metering_3,col="blue")
legend("topright",lty=1,col=c("black","red","blue"),legend=c("Sub_metering_1","Sub_metering_2","Sub_metering_3"))
plot(DF10$DFdate,DF10$Global_reactive_power,type="l",ylab="Global_reactive_power",xlab="datetime")
