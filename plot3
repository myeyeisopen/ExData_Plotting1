plot(DF10$DFdate,DF10$Sub_metering_1,type="n",ylab="Energy sub metering",xlab="")
lines(DF10$DFdate,DF10$Sub_metering_1)
lines(DF10$DFdate,DF10$Sub_metering_2,col="red")      
lines(DF10$DFdate,DF10$Sub_metering_3,col="blue")
legend("topright",lty=1,legend=c("Sub_metering_1","Sub_metering_2","Sub_metering_3"),col=c("black","red","blue"))
